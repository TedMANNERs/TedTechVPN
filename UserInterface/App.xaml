<Application x:Class="TedTechVpn.UserInterface.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:TedTechVpn.UserInterface.ViewModels"
             xmlns:userInterface="clr-namespace:TedTechVpn.UserInterface"
             StartupUri="MainView.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:AppViewModel}">
            <userInterface:AppView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:LoginViewModel}">
            <userInterface:LoginView/>
        </DataTemplate>
    </Application.Resources>
</Application>
